<h1>Listing posts</h1>

<table>
  <tr>
    <th>Feed</th>
    <th>Summary</th>
    <th>Content</th>
    <th>Author</th>
    <th>Rank</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.feed_id %></td>
    <td><%= post.summary %></td>
    <td><%= post.content %></td>
    <td><%= post.author %></td>
    <td><%= post.rank %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
